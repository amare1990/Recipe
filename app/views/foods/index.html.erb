<p style="color: green"><%= notice %></p>

<h1>Foods</h1>

<div id="foods">
  <% @foods.each do |food| %>
    <%= render food %>
    <p>
      <%= link_to "Show this food", user_food_path(current_user, food) %>
    </p>
  <% end %>
</div>

<%= link_to "New food", new_user_food_path(current_user) %>
